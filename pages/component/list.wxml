<!--pages/component/list.wxml-->
<view class='list' wx:for="{{innerData}}">
<view class="inner" bindtap='to'>
  <image src='{{innerUrl}}{{item.img}}'></image>
  <view class='content'>
    {{item.name}}
  </view>
  <view class='footer'>
    <text>￥{{item.price}}</text>
    <text>{{item.goodPraise}}人已付款</text>
  </view>
</view>
</view>
